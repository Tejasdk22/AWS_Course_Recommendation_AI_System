# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# AWS Bedrock Configuration
BEDROCK_MODEL_ID=amazon.titan-text-express-v1
# Alternative models (no use case required):
# - amazon.titan-text-express-v1 (recommended)
# - amazon.titan-text-lite-v1 (smaller, faster)
# - amazon.titan-embed-text-v1 (for embeddings)

# AWS Bedrock Agent Core Configuration
USE_BEDROCK_AGENT_CORE=false
BEDROCK_AGENT_ID=ag-xxxxxxxx
BEDROCK_AGENT_ALIAS_ID=ag-alias-xxxxxxxx

# Application Configuration
LOG_LEVEL=INFO
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=30
